{
  "name": "amis-editor-wc-cra",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "amis": "^2.0.0",
    "amis-editor": "^5.1.0",
    "axios": "^1.0.0-0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "devDependencies": {
    "@craco/craco": "^7.0.0-alpha.7",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "craco start",
    "dev": "react-scripts start",
    "build": "craco build",
    "test": "craco test",
    "serve": "npx -y serve -s build",
    "surge": "cp build/index.html build/200.html && pnpx surge build amis-editor.surge.stevie.top",
    "gh": "cp build/index.html build/404.html && npx -y gh-pages -d build -b gh-pages"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "import/no-webpack-loader-syntax": 0
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
